// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Currency {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  name      String
  fullName  String
  buy       Int
  sell      Int
  image     String
}
